    "version": "0.2.0",
    "configurations": [
        {
            "name": "Plot Per File",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/report/plots_per_file.py",
            "cwd": "${fileDirname}",
            "console": "integratedTerminal"
        },
        {
            "name": "Plot Progressive",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/report/plots_progressive.py",
            "cwd": "${fileDirname}",
            "console": "integratedTerminal"
        },
        {
            "name": "Report-Converter",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/report/reportToMarkdown.py",
            "cwd": "${fileDirname}",
            "console": "integratedTerminal"
        },
        {
            "name": "Test",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/tests/LZ_Collection_tst",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Compress Per File",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/source/LZ_Collection",
            "args": [
                "all",
                "dummy_res",
                "all",
                "-c",
                "-b"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Compress Progressive",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/source/LZ_Collection",
            "args": [
                "data/proteins.50MB",
                "dummy_res",
                "Approx.LZ77Par",
                "-c",
                "-bp=10"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Benchmark Speedup",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/source/LZ_Collection",
            "args": [
                "data/proteins.50MB",
                "dummy_res",
                "Approx.LZ77Par",
                "-c",
                "-bs"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}